<table>
    <tr>
        <td>id</td>
        <td>name</td>
        <td>roll</td>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Roll</td>
            <td><a asp-action="Update" asp-route-id="@item.Id">Update</a></td>
            <td><a asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
        </tr>
    }
</table>

<a asp-action="Create">Create Student</a>